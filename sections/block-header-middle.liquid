{%- style -%}
	.block-header-middle {
		background-color: var(--theme-color-brown); color:var(--theme-color-white);
		& > .block-header-middle-content {
			display:flex; flex-wrap: nowrap; flex-direction: row; justify-content: space-between; align-items: center; column-gap: 2em; height:3em;
			  & > figure#site-logo {
				flex-basis:8em; margin: 0; padding: 0;
				position: relative;
				& > a {
				  display:block; position: absolute; left:0; top:0; right:0; bottom:0; width:100%; height:100%; z-index: 3;
				}
			  }
			  & > nav#short-menu {
				margin-right: auto;
				& ul {
				  list-style: none; padding:0; margin: 0;
				  & > li {
					& > a { text-decoration: none;}
				  }
				}
				& > ul {
				  display:flex; flex-wrap: nowrap; flex-direction: row; justify-content: space-between; align-items: center; column-gap: 1em;
				  & > li {
					& > a {
					  color:#fff;
					}
				  }
				}
			  }
			  & > .header__search {
				& summary {
				  color:#fff;
				}
			  }
			  & > nav#user-menu {
				& ul {
					display:flex; flex-wrap: nowrap; flex-direction: row; justify-content: space-between; align-items: center; column-gap: 1em;
				  & li {
					& a { 
						display:block;
						color: #fff; font-size: 1em; position: relative;
						& > i { 
							flex-basis: 42px; color:#fff; font-style: normal;
							& > svg {
								width: 1.4em; height:1.4em; transform: none;
							}
						}
						& > span {}
						& .cart-count-bubble {
							background-color: var(--theme-color-orange); right:0.4em; top:-0.4em; font-size: 0.8em;
						}
					}
				  }
				}
			  }
		}
	}
{%- endstyle -%}

